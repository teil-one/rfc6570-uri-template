{
  "name": "rfc6570-uri-template",
  "version": "0.4.0",
  "description": "RFC 6570 URI Template Parser",
  "main": "index.js",
  "types": "index.d.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.js"
    }
  },
  "scripts": {
    "build:grammar": "shx mkdir -p ./build && peggy ./src/grammar.pegjs --format es -o ./build/grammar.js",
    "build:app": "tsc && shx cp ./src/*.d.ts ./build",
    "bundle": "rollup -c rollup.config.js",
    "build": "rimraf lib && rimraf build && npm run build:grammar && npm run build:app && npm run bundle",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "jest": "peggy ./src/grammar.pegjs --format es -o ./src/grammar.js && cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "pretest": "npm run lint && npm run prettier:check",
    "test": "npm run jest",
    "prepublishOnly": "npm test",
    "prepare": "npm run build && shx ln -f lib/index.js index.js && shx ln -f lib/index.d.ts index.d.ts && shx ln -f lib/index.cjs index.cjs",
    "postpack": "shx rm index.js && shx rm index.d.ts && shx rm index.cjs",
    "preversion": "npm test"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "index.cjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teil-one/rfc6570-uri-template.git"
  },
  "keywords": [
    "rfc6570",
    "uri",
    "url",
    "template"
  ],
  "author": "Sergei Koshel",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/teil-one/rfc6570-uri-template/issues"
  },
  "homepage": "https://github.com/teil-one/rfc6570-uri-template#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.2.0",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "peggy": "^2.0.1",
    "prettier": "2.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.3",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "volta": {
    "node": "18.16.0"
  }
}
